#!/bin/bash

# Run the container.
# 		-d: Run the container in the background.
# 		-P: Map network ports inside container to host.
#	--name: Set the name of the container.
#		-v: Mount host volumes to container.
docker run -d -P --name noughtsandcrosses-container -v `pwd`/web:/srv/www/noughtsandcrosses/web noughtsandcrosses-image
